{
    "name": "abc/job-server-bundle",
    "type": "symfony-bundle",
    "description": "A symfony bundle to manage jobs for asynchronous distributed processing using AbcJobWorkerBundle",
    "keywords": ["job", "asynchronous", "distributed", "schedule", "cron", "queue"],
    "homepage": "http://aboutcoders.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Hannes Schulz",
            "email": "hannes.schulz@aboutcoders.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "psr/log": "^1.0",
        "symfony/framework-bundle": "^3.4|^4",
        "abc/job": "dev-route-api"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "symfony/browser-kit": "^3.4|^4",
        "symfony/yaml": "^3.4|^4",
        "doctrine/doctrine-bundle": "^1.3",
        "doctrine/orm": "^2.6",
        "enqueue/enqueue-bundle": "^0.9",
        "aboutcoders/scheduler-bundle": "2.x-dev",
        "enqueue/null": "^0.9"
    },
    "autoload": {
        "psr-4": { "Abc\\JobServerBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
