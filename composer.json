{
    "name": "abc/job-server-bundle",
    "type": "symfony-bundle",
    "description": "A symfony bundle to manage asynchronous distributed processing of stateful jobs",
    "keywords": ["job", "asynchronous", "distributed", "schedule", "cron", "queue"],
    "homepage": "http://aboutcoders.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Hannes Schulz",
            "email": "hannes.schulz@aboutcoders.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "psr/log": "^1.0",
        "symfony/framework-bundle": "^4.3|^5",
        "abc/job": "^0.2|dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "symfony/browser-kit": "^4.3|5",
        "symfony/yaml": "^4.3|5",
        "dama/doctrine-test-bundle": "^6.2",
        "doctrine/orm": "^2.7",
        "doctrine/doctrine-bundle": "~1.2|^2",
        "enqueue/enqueue-bundle": "^0.10",
        "enqueue/null": "^0.10",
        "aboutcoders/scheduler-bundle": "2.x-dev"
    },
    "autoload": {
        "psr-4": { "Abc\\JobServerBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
